    "axios" : "^0.13.1",
    "babel-cli" : "^6.10.1",
    "babel-core" : "^6.10.4",
    "babel-preset-es2015" : "^6.9.0",
    "babel-preset-stage-2" : "^6.11.0",
    "bluebird" : "^3.4.1",
